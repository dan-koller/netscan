<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net8.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<!-- Change dll & executable name -->
		<TargetName>NetScan</TargetName>
		<AssemblyName>NetScan</AssemblyName>
	</PropertyGroup>

	<ItemGroup>
	  <ProjectReference Include="..\NScan.Core\NScan.Core.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Using Include="System.Console" Static="true" />
	</ItemGroup>

</Project>
